{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [
            2,
            "tab",
            {
                "SwitchCase": 1
            }
        ],
        "strict": 0,
        "no-tabs":"off",
        "no-var": "error",
        "no-console": 0,
        "no-fallthrough": 0
    }
}